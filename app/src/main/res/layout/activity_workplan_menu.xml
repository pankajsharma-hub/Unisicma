                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:paddingLeft="16dp"
                    android:text="TextView"
                    android:textSize="16sp" />
            </LinearLayout>
        </androidx.cardview.widget.CardView>


        <!---........................................8th Row - .................................................-->


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="6dp"
            app:cardCornerRadius="8dp"
            app:cardElevation="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#4B9CD5"
                android:gravity="center"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/h_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:text="@string/h_mobile"
                    android:textSize="16sp"
                    android:textStyle="bold"

                    />

                <TextView
                    android:id="@+id/fMobile_value"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:paddingLeft="16dp"
                    android:text="TextView"
                    android:textSize="16sp" />
            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <!---........................................9th Row - .................................................-->


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="6dp"
            app:cardCornerRadius="8dp"
            app:cardElevation="8dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#4B9CD5"
                android:gravity="center"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/vaccine_received"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:text="@string/v_received"
                    android:textSize="16sp"
                    android:gravity="center_horizontal"
                    android:textStyle="bold"

                    />
               <!-- <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:id="@+id/vaccine_space"
                    android:layout_margin="5dp"
                    android:layout_toEndOf="@+id/vaccine_received"
                    android:layout_toRightOf="@+id/vaccine_received"/>-->


                <TextView
                    android:id="@+id/received_value"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="3dp"
                    android:layout_below="@+id/vaccine_received"
                    android:text=""
                    android:textSize="16sp" />

              <!--  <View
                    android:id="@+id/view"
                    android:layout_width="1dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="1dp"
                    android:layout_toEndOf="@+id/received_value"
                    android:layout_toRightOf="@+id/received_value"
                    android:background="@color/cardview_shadow_start_color" />-->

                <TextView
                    android:id="@+id/received_date"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="3dp"
                    android:layout_toEndOf="@+id/vaccine_received"
                    android:layout_toRightOf="@+id/vaccine_received"
                    android:layout_below="@+id/vaccine_received"
                    android:text=""
                    android:textSize="16sp" />
            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <!---........................................10th Row - .................................................-->


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="6dp"
            app:cardCornerRadius="8dp"
            app:cardElevation="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#4B9CD5"
                android:gravity="center"
                android:orientation="vertical">


                <TextView
                    android:id="@+id/vaccine_remained"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:text="@string/remained"
                    android:textSize="16sp"
                    android:textStyle="bold"

                    />

                <TextView
                    android:id="@+id/remained_value"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:paddingLeft="16dp"
                    android:text=""
                    android:textSize="16sp" />
            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <!---........................................11th Row - .................................................-->


        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="6dp"
            app:cardCornerRadius="8dp"
            app:cardElevation="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#4B9CD5"
                android:gravity="center"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/dueDate"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:text="@string/dueDate"
                    android:textSize="16sp"
                    android:textStyle="bold"

                    />

                <TextView
                    android:id="@+id/date_value"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="5dp"
                    android:paddingLeft="16dp"
                    android:text=""
                    android:textSize="16sp" />
            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <!---........................................12th Row - .................................................-->


        <!---........................................13th Row - .................................................-->


